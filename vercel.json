{
  "buildCommand": "ng build --configuration production --base-href /",
  "outputDirectory": "dist/angular-primeng/browser",
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}