{
  "version": 2,
  "public": false,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/angular-primeng/browser",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}